<div class="content">
<h1><%= @zone.name %></h1>
<h3><%= @zone.description %></h3>
<img src=<%=@zone.img_url%>>


<%@zone.pokemons.each do |pokemon|%>
<%=link_to pokemon.name, pokemon_path(pokemon)%>

<a href=<%=pokemon_path(pokemon)%>><img src=<%= pokemon.img_url%>></a>
<%end%><br><br><br>
<%=link_to "Back Profile", user_path(session[:id])%><br>
<%=link_to "Back To Pokedex", pokemons_path%><br><br><br><br>
</div>